{
  "build": {
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "kartof-music",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "window": {
        "all": true,
        "create": false
      }
    },
    "windows": [
      {
        "decorations": false,
        "transparent": true,
        "fullscreen": false,
        "resizable": true,
        "title": "kartof-music",
        "width": 800,
        "height": 600,
        "minWidth": 700
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "resources": ["audios"],
      "targets": "all",
      "identifier": "com.kartof.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
